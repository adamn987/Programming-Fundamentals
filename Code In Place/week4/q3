#include <stdio.h>
int main(void) {
    int units;
    float bill;
    printf("Enter units consumed: ");
    scanf("%d", &units);
    if (units <= 100) {
        bill = units * 5;
    } else if (units <= 200) {
        bill = 100 * 5 + (units - 100) * 7;
    } else {
        bill = (100 * 5 + 100 * 7 + (units - 200) * 10);
    }
    printf("electricity bill is %f \n", bill);
}
//Calculating what the electricity is based  on number of units
