//Sum is Even
#include<stdio.h>
  int main(){
    int n;
//Taking Input
    printf("Enter number");
    scanf("%d" , &n);

//Using Nested Loops
    for(int i=1 ; i<=n ; i++) {
        for(int j=1 ; j<=n ; j++) {
            if( (i+j) %2 == 0) {
                printf("(%d, %d)\n" ,i, j);
         }
      }
    }
  
    return 0;
}
